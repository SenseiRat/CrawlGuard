{
  "manifest_version": 3,
  "name": "CrawlGuard",
  "version": "0.1",
  "permissions": ["contextMenus","scripting","activeTab"],
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["contentScript.js"]
    }
  ]
}
